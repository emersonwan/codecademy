my_list = [i**2 for i in range(1,11)]
# Generates a list of squares of the numbers 1 - 10

f = open("output.txt", "w") # Cria o arquivo output.txt

for item in my_list:
    f.write(str(item) + "\n") # Escreve no arquivo output.txt
    # os n√∫meros da lista my_list quebrando linha

f.close() # Fecha o arquivo output.txt
