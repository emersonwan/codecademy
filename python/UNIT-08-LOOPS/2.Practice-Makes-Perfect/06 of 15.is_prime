def is_prime(x):
    if x < 2:
        return False
    elif x == 2:
        return True
    else:
        for n in range(2, x):
            print x, "%", n, "=", x % n
            if x % n == 0:
                return False
        else:
            return True

print is_prime(1)
print is_prime(2)
print is_prime(3)
print is_prime(4)
print is_prime(5)
print is_prime(113)
print is_prime(121)
