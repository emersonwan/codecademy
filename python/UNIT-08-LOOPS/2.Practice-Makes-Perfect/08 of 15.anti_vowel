def anti_vowel(text):
    string = ""
    for char in text:
        if char not in "AEIOUaeiou":
            string += char
    return string

print anti_vowel("Hey You!")
print anti_vowel("ABCDPython")
print anti_vowel("Adam Euro Ino Onix Utopia")
